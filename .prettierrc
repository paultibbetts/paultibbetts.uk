{
  "plugins": ["prettier-plugin-go-template"],
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "parser": "go-template"
      }
    },
    {
      "files": ["*.md"],
      "options": {
        "proseWrap": "preserve"
      }
    }
  ],
  "printWidth": 100,
  "tabWidth": 2,
  "singleQuote": false,
  "semi": false,
  "trailingComma": "none"
}
